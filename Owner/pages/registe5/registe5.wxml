                <view class="shijianhang">
                    <text class='{{rilicss_1[index]==0?"rilicss0":"rilicss1"}}' wx:for="{{shijian}}" bindtap="djrili1" wx:for-index="index" data-index="{{index}}"></text>
                </view>
                <view class="shijianhang">
                    <text class='{{rilicss_2[index]==0?"rilicss0":"rilicss1"}}' wx:for="{{shijian}}" bindtap="djrili2" wx:for-index="index" data-index="{{index}}"></text>
